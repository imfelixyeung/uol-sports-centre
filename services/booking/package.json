{
  "name": "booking",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --testPathPattern=src/tests/unit",
    "test:integration": "jest --testPathPattern=src/tests/integration",
    "posttest": "pnpm lint",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "lint": "pnpm check-types && gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "check-types": "tsc --noemit",
    "db:push": "prisma db push",
    "seed": "prisma db seed",
    "studio": "prisma studio",
    "dev": "pnpm db:push && pnpm seed && tsx watch --clear-screen=false src/index.ts",
    "build": "swc ./src -d build",
    "serve": "pnpm db:push && pnpm seed && node build/index.js",
    "ci": "pnpm db:push && pnpm seed && pnpm lint && pnpm test && pnpm test:integration"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/jsonwebtoken": "^9.0.1",
    "axios": "^1.3.4",
    "axios-cache-interceptor": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.40",
    "@swc/jest": "^0.2.24",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.3",
    "@types/supertest": "^2.0.12",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.3",
    "prisma": "^4.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5"
  }
}
